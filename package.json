{
  "name": "portals",
  "version": "0.1.0",
  "homepage": "/portals",
  "private": true,
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
    ],
    "globalSetup": "./src/testE2E/setup.js",
    "globalTeardown": "./src/testE2E/teardown.js"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "antd": "^4.6.1",
    "antd-img-crop": "^3.10.0",
    "axios": "^0.21.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.43.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:e2e": "react-scripts test --testPathPattern=.*e2e.test.js$ --env=./src/testE2E/puppeteer_environment.js",
    "eject": "react-scripts eject"
